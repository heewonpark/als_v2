public CalcPeakISF
func CalcPeakISF(){local i, tmp localobj spt
    spt = $o1
    PeakISF = -1
    //spt.printf("%f\t")
    //printf("\n")
    for i=0, spt.size()-1{
	if(i>0){
	    interval = spt.x[i]-spt.x[i-1]
	    ISF = 1000.0/interval
	    //print ISF
	    if(ISF>PeakISF){
		PeakISF = ISF
		//printf("PeakISF : %f\n",PeakISF)
	    }
	}
    }
    return PeakISF
}

